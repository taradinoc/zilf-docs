###  http://example.org/zilf/BitOn
zilf:BitOn rdf:type owl:NamedIndividual ,
                    skos:Concept ;
           skos:broader zilf:PropFlags ;
           skos:definition "Flag indicating that a light-source object is currently providing light." ;
           skos:prefLabel "ONBIT"@en .


###  http://example.org/zilf/BitTake
zilf:BitTake rdf:type owl:NamedIndividual ,
                      skos:Concept ;
             skos:broader zilf:PropFlags ;
             skos:definition "Flag indicating that an object can be picked up by the player." ;
             skos:prefLabel "TAKEBIT"@en .


###  http://example.org/zilf/CoreFunctions
zilf:CoreFunctions rdf:type owl:NamedIndividual ,
                            skos:Concept ;
                   skos:narrower zilf:EVAL ,
                                 zilf:PNAME ,
                                 zilf:ROOT ,
                                 zilf:SET ,
                                 zilf:SETG ,
                                 zilf:SETG20 ,
                                 zilf:SPNAME ,
                                 zilf:STRING ;
                   skos:topConceptOf zilf:ZILF_Scheme ;
                   skos:prefLabel "Core Functions"@en .


###  http://example.org/zilf/EVAL
zilf:EVAL rdf:type owl:NamedIndividual ,
                   skos:Concept ;
          skos:inScheme zilf:ZILF_Scheme ;
          skos:definition "MDL built-in that evaluates value (usually a FORM created by FORM or QUOTE)."@en ;
          skos:editorialNote "It is possible to supply an environment for EVAL. This tells EVAL from which environment. EVAL should take variable bindings. See The MDL Programming Language, chap. 9.7 for more about the environment."@en ;
          skos:example """<SET F '<+ 1 2>> 
.F ; Sets up the stack operator and operands, + 1 2.
<EVAL .F> ; Evaluates the operands with the given operator from the stack to 3.
<SET A 0>
<DEFINE WRONG ('B \"AUX\" (A 1)) <EVAL .B>> 
<DEFINE RIGHT (\"BIND\" E 'B \"AUX\" (A 1)) <EVAL .B .E>> 
<WRONG .A> ; 1
<RIGHT .A> ; 0"""@en ;
          skos:prefLabel "EVAL"@en .


###  http://example.org/zilf/OBJECT
zilf:OBJECT rdf:type owl:NamedIndividual .


###  http://example.org/zilf/ObjectProperties
zilf:ObjectProperties rdf:type owl:NamedIndividual ,
                               skos:Concept ;
                      skos:broader zilf:ObjectSystem ;
                      skos:narrower zilf:PropAdjective ,
                                    zilf:PropDesc ,
                                    zilf:PropFlags ,
                                    zilf:PropSynonym ;
                      skos:prefLabel "Object Properties"@en .


###  http://example.org/zilf/ObjectSystem
zilf:ObjectSystem rdf:type owl:NamedIndividual ,
                           skos:Concept ;
                  skos:narrower zilf:OBJECT ,
                                zilf:ObjectProperties ;
                  skos:topConceptOf zilf:ZILF_Scheme ;
                  skos:prefLabel "Object System"@en .


###  http://example.org/zilf/PNAME
zilf:PNAME rdf:type owl:NamedIndividual ,
                    skos:Concept ;
           skos:inScheme zilf:ZILF_Scheme ;
           skos:definition "MDL built-in for \"printed name\", which returns a newly created string copy of the atom's pname."@en ;
           skos:editorialNote "PNAME never prints an ATOMs trailers, unlike UNPARSE, and is therefore quicker."@en ;
           skos:example """<PNAME FOO> ; \"FOO\"
<PNAME FOO!-NEW-OBLIST> ; \"FOO\"
<UNPARSE FOO!-NEW-OBLIST> ; \"FOO!-NEW-OBLIST\""""@en ;
           skos:prefLabel "PNAME"@en .


###  http://example.org/zilf/ParserSystem
zilf:ParserSystem rdf:type owl:NamedIndividual ,
                           skos:Concept ;
                  skos:narrower zilf:SYNTAX ,
                                zilf:VERB ;
                  skos:topConceptOf zilf:ZILF_Scheme ;
                  skos:prefLabel "Parser System"@en .


###  http://example.org/zilf/PropAdjective
zilf:PropAdjective rdf:type owl:NamedIndividual ,
                            skos:Concept ;
                   skos:broader zilf:ObjectProperties ;
                   skos:definition "A list of adjectives the parser accepts to identify a specific object." ;
                   skos:example "ADJECTIVE BRASS SMALL" ;
                   skos:prefLabel "ADJECTIVE"@en .


###  http://example.org/zilf/PropDesc
zilf:PropDesc rdf:type owl:NamedIndividual ,
                       skos:Concept ;
              skos:broader zilf:ObjectProperties ;
              skos:definition "A short string used to name the object in game output." ;
              skos:prefLabel "DESC"@en .


###  http://example.org/zilf/PropFlags
zilf:PropFlags rdf:type owl:NamedIndividual ,
                        skos:Concept ;
               skos:broader zilf:ObjectProperties ;
               skos:narrower zilf:BitOn ,
                             zilf:BitTake ;
               skos:definition "A collection of boolean bits (flags) that define the state or behavior of an object." ;
               skos:prefLabel "FLAGS"@en .


###  http://example.org/zilf/PropSynonym
zilf:PropSynonym rdf:type owl:NamedIndividual ,
                          skos:Concept ;
                 skos:broader zilf:ObjectProperties ;
                 skos:definition "A list of nouns the parser accepts for this object." ;
                 skos:prefLabel "SYNONYM"@en .


###  http://example.org/zilf/ROOT
zilf:ROOT rdf:type owl:NamedIndividual ,
                   skos:Concept ;
          skos:inScheme zilf:ZILF_Scheme ;
          skos:definition "MDL built-in that returns the OBLIST containing names of primitives (the same as <2 .OBLIST>)."@en ;
          skos:editorialNote "Initially contains all predefined SUBRs or FSUBRs, as well as OBLIST, DEFAULT, T, etc."@en ;
          skos:example "<ROOT>"@en ;
          skos:prefLabel "ROOT"@en .


###  http://example.org/zilf/SET
zilf:SET rdf:type owl:NamedIndividual ,
                  skos:Concept ;
         skos:inScheme zilf:ZILF_Scheme ;
         skos:definition "MDL built-in that asigns a value to the local atom."@en ;
         skos:editorialNote "It is possible to supply an environment for SET. See EVAL for more about the environment."@en ;
         skos:example "<PROG (X) <SET X 5> <RETURN .X>> ; Returns 5 after setting X."@en ;
         skos:prefLabel "SET"@en .


###  http://example.org/zilf/SETG
zilf:SETG rdf:type owl:NamedIndividual ,
                   skos:Concept ;
          skos:broader zilf:CoreFunctions ;
          skos:definition "MDL built-in that assigns a value to the global atom. If an atom is already assigned a value, it is changed."@en ;
          skos:example "<SETG MYVAR 42> ; Store 42 in global atom MYVAR"@en ;
          skos:prefLabel "SETG"@en .


###  http://example.org/zilf/SETG20
zilf:SETG20 rdf:type owl:NamedIndividual ,
                     skos:Concept ;
            skos:broader zilf:CoreFunctions ;
            skos:inScheme zilf:ZILF_Scheme ;
            skos:definition "A member of the ZIL library, which assigs a value to the global atom. If an atom is already assigned a value, it is changed. SETG20 is an alias for SETG."@en ;
            skos:example "<SETG20 MYVAR 42> ; Store 42 in global atom MYVAR"@en ;
            skos:prefLabel "SETG20"@en .


###  http://example.org/zilf/SPNAME
zilf:SPNAME rdf:type owl:NamedIndividual ,
                     skos:Concept ;
            skos:inScheme zilf:ZILF_Scheme ;
            skos:definition "MDL built-in for \"shared printed name\", which should return the same string of the atom's pname that is in its OBLIST (i.e. pointing to the same storage and therefore not able to change or modify)."@en ;
            skos:editorialNote "ZILF treats SPNAME as an alias to PNAME and returns a string copy of the atomâ€™s pname."@en ;
            skos:example "<SPNAME atom>"@en ;
            skos:prefLabel "SPNAME"@en .


###  http://example.org/zilf/STRING
zilf:STRING rdf:type owl:NamedIndividual ,
                     skos:Concept ;
            skos:inScheme zilf:ZILF_Scheme ;
            skos:definition "MDL built-in that returns a concatenated string of all values. values can be character or string."@en ;
            skos:editorialNote "A string is a block of contiguous bytes where each byte holds a character. See more about STRING  structure in The MDL Programming Language, Appendix 1."@en ;
            skos:example "<STRING !\\A <ASCII 66> \"CD\"> ; \"ABCD\""@en ;
            skos:prefLabel "STRING"@en .


###  http://example.org/zilf/SYNTAX
zilf:SYNTAX rdf:type owl:NamedIndividual ,
                     skos:Concept ;
            skos:broader zilf:ParserSystem ;
            skos:prefLabel "SYNTAX"@en .


###  http://example.org/zilf/VERB
zilf:VERB rdf:type owl:NamedIndividual ,
                   skos:Concept ;
          skos:broader zilf:ParserSystem ;
          skos:prefLabel "VERB"@en .


###  http://example.org/zilf/ZILF_Scheme
zilf:ZILF_Scheme rdf:type owl:NamedIndividual ,
                          skos:ConceptScheme ;
                 skos:hasTopConcept zilf:CoreFunctions ,
                                    zilf:ObjectSystem ,
                                    zilf:ParserSystem ;
                 skos:prefLabel "ZILF Reference Scheme"@en .



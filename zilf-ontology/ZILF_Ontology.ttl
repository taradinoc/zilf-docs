                   skos:Concept ;
          skos:broader zilf:CoreFunctions ;
          skos:inScheme zilf:ZILF_Scheme ;
          skos:definition "MDL built-in function that is used for measuring CPU execution time, typically used for debugging ZILCH."@en ;
          skos:editorialNote "ZILF ignores this and always returns 1, because modern IDE's offer different methods of debugging ZILCH."@en ;
          skos:example "<TIME>"@en ;
          skos:prefLabel "<TIME>"@en .


###  http://example.org/zilf/TOP
zilf:TOP rdf:type owl:NamedIndividual ,
                  skos:Concept ;
         skos:broader zilf:CoreFunctions ;
         skos:inScheme zilf:ZILF_Scheme ;
         skos:definition "MDL built-in function that returns array with all elements put back in  array. TOP only works on the structures VECTOR or STRING (arrays) and not on a LIST (a LIST is only pointing forward)."@en ;
         skos:editorialNote "The returned array is not a copy but pointing to the same array with another starting element. Also see BACK, NTH, PUT, REST and SUBSTRUC."@en ;
         skos:example """<SETG STRUCT1 [1 2 3 4 5]>  ;  STRUCT1 = [1 2 3 4 5]
<SETG STRUCT2 <REST ,STRUCT1 2>>  ;  STRUCT2 = [3 4 5]
<TOP ,STRUCT2>  ;  STRUCT2 = [1 2 3 4 5]"""@en ;
         skos:prefLabel "<TOP array>"@en .


###  http://example.org/zilf/VALUE
zilf:VALUE rdf:type owl:NamedIndividual ,
                    skos:Concept ;
           skos:broader zilf:CoreFunctions ;
           skos:inScheme zilf:ZILF_Scheme ;
           skos:definition "MDL built-in function that returns the value of an atom. If  the atom has an LVAL then the LVAL is returned, otherwise the GVAL of the atom is returned."@en ;
           skos:editorialNote "It is possible to supply an environment for VALUE. See EVAL for more about the environment."@en ;
           skos:example """<SETG X 3>
<SET X 4>
<VALUE X>  ;  4
<UNASSIGN X>
<VALUE X>  ;  3"""@en ;
           skos:prefLabel "<VALUE atom [environment]>"@en .



                                       skos:Concept ;
                              skos:broader zilf:CoreFunctions ;
                              skos:inScheme zilf:ZILF_Scheme ;
                              skos:definition "MDL built-in function that returns the same result as OR with the difference that all exressions are evaluated."@en ;

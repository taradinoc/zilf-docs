            skos:example """<PUSH 42>
<RSTACK>  ;  Returns 42"""@en ;
            skos:historyNote "Zapf syntax: RSTACK, Inform syntax: ret_popped"@en ;
            skos:prefLabel "<RSTACK>"@en .



###  http://example.org/zilf/COMPILATION-FLAG
zilf:COMPILATION-FLAG rdf:type owl:NamedIndividual ,
                               skos:Concept ;
                      skos:broader zilf:CoreFunctions ;
                      skos:inScheme zilf:ZILF_Scheme ;
                      skos:definition """ZIL library function that defines a COMPILATION-FLAG named atom-or-string initialized to value. If no value is supplied it defaults to TRUE. The name of the flag can either be an ATOM or a STRING whose text becomes the ATOM. 

The flag can then be read by COMPILATION-FLAG-VALUE or used as a condition in 
IFFLAG."""@en ;
                      skos:editorialNote "A call to COMPILATION-FLAG with an already defined ATOM changes the value of the ATOM."@en ;
                      skos:example """<COMPILATION-FLAG MYFLAG>
<COMPILATION-FLAG-VALUE MYFLAG>  ;  T
<COMPILATION-FLAG “MYFLAG” 123>
<COMPILATION-FLAG-VALUE MYFLAG>  ;  123"""@en ;
                      skos:prefLabel "<COMPILATION-FLAG atom-or-string [value]>"@en .


###  http://example.org/zilf/COMPILATION-FLAG-DEFAULT
zilf:COMPILATION-FLAG-DEFAULT rdf:type owl:NamedIndividual ,
                                       skos:Concept ;
                              skos:broader zilf:CoreFunctions ;
                              skos:inScheme zilf:ZILF_Scheme ;
                              skos:definition """ZIL library function that defines a COMPILATION-FLAG named atom-or-string initialized to value. If no value is supplied it defaults to TRUE. The name of the flag can either be an ATOM or a STRING whose text becomes the ATOM. 

The flag can then be read by COMPILATION-FLAG-VALUE or used as a condition in IFFLAG."""@en ;
                              skos:editorialNote "A call to COMPILATION-FLAG-DEFAULT with an already defined ATOM doesn't change the value of the ATOM."@en ;
                              skos:example """<COMPILATION-FLAG-DEFAULT MYFLAG T>
<COMPILATION-FLAG-VALUE MYFLAG>  ;  T 
<COMPILATION-FLAG “MYFLAG” 123>
<COMPILATION-FLAG-VALUE MYFLAG>  ;  123 
<COMPILATION-FLAG-DEFAULT MYFLAG T>
<COMPILATION-FLAG-VALUE MYFLAG>  ;  123"""@en ;
                              skos:prefLabel "<COMPILATION-FLAG-DEFAULT atom-or-string value>"@en .


###  http://example.org/zilf/COMPILATION-FLAG-VALUE
zilf:COMPILATION-FLAG-VALUE rdf:type owl:NamedIndividual ,
                                     skos:Concept ;
                            skos:broader zilf:CoreFunctions ;
                            skos:inScheme zilf:ZILF_Scheme ;
                            skos:definition "ZIL library function that returns the value of the COMPILATION-FLAG atom-or-string. If no atom-or-string is defined it returns FALSE."@en ;
                            skos:example """<COMPILATION-FLAG MYFLAG 123>
<COMPILATION-FLAG-VALUE MYFLAG>  ;  123
<COMPILATION-FLAG-VALUE ASDFGHJKL>  ;  #FALSE"""@en ;
                            skos:prefLabel "<COMPILATION-FLAG-VALUE atom-or-string>"@en .


###  http://example.org/zilf/CONS
zilf:CONS rdf:type owl:NamedIndividual ,
                   skos:Concept ;
          skos:broader zilf:CoreFunctions ;
          skos:inScheme zilf:ZILF_Scheme ;
          skos:definition "MDL built-in function that adds first to the front of list, without copying list, and returns the resulting LIST. References to list are not affected."@en ;
          skos:editorialNote "CONS means \"construct\"."@en ;
          skos:example """<CONS 1 (2 3)>  ;  (1 2 3)
<SET S1 (!\\B !\\C)>
<SET S2 <CONS !\\A .S1>>
<PUT .S1 2 !\\D>
.S2  ;  (!\\A !\\B !\\D)"""@en ;
          skos:prefLabel "<CONS first list>"@en .


###  http://example.org/zilf/CONSTANT
zilf:CONSTANT rdf:type owl:NamedIndividual ,
                       skos:Concept ;
              skos:broader zilf:CoreFunctions ;
              skos:inScheme zilf:ZILF_Scheme ;
              skos:definition "ZIL library function that defines an atom with value that will never be changed. The atom can is accessed inside a ROUTINE with GVAL (or ,) just like a GLOBAL atom. Defining a CONSTANT instead of a GLOBAL when possible can be vital information the compiler can use for optimization."@en ;
              skos:editorialNote "MSETG is an alias for CONSTANT."@en ;
              skos:example """<CONSTANT MSG-CANT-DO-THAT \"You can't do that!\">
...
<TELL ,MSG-CANT-DO-THAT CR>"""@en ;
              skos:prefLabel "<CONSTANT atom value>"@en .


